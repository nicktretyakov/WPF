<prism:PrismApplication x:Class="Ecours.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/MainTabItem.xaml"/>
                <ResourceDictionary Source="Resources/InnerTabItem.xaml"/>
                <ResourceDictionary Source="Resources/MainTabBtn.xaml"/>

                <ResourceDictionary Source="Resources/BtnStyles.xaml"/>
                <ResourceDictionary Source="Resources/DlgStyles.xaml"/>
                <ResourceDictionary Source="Resources/DefaultStyles.xaml"/>
                <ResourceDictionary Source="Resources/LabelStyles.xaml"/>
                <ResourceDictionary Source="Resources/ChStyles.xaml"/>
                <ResourceDictionary Source="Resources/RbStyles.xaml"/>
                <ResourceDictionary Source="Resources/TextBoxStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
